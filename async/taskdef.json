{
  "executionRoleArn": "arn:aws:iam::501045672667:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "bdux-example-async",
      "image": "<CONTAINER_IMAGE>",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 8080,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "bdux-example-async",
  "revision": 8
}
